rule all_qiime_demux_stat:
    input:
        TARGET_DEMUX_STAT

rule get_demux_stat:
    input:
        QIIME_OUTPUT_DIR + "/demux.qzv"
    output:
        QIIME_OUTPUT_DIR + "/demux_stat"
    shell:
        """
        qiime tools export \
        --input-path {input} \
        --output-path {output}
        """
